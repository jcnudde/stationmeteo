//---------------------------------------------------------------------------

#pragma hdrstop

#include "Barometre.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)

Barometre::Barometre(double channel):Capteur(channel)
{

}
double Barometre::readValue()
{
    this->carte->readVoltage(this->channel);
    return 2000;
}
string Barometre::getUnit()
{
    return "Pa";
}
